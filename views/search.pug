extends layouts/main.pug
include mixins/videoBlock.pug

block content
    .search__header
        h3 Searching for #{searchingBy}
    .search__videos
        if videos.length === 0
            h5 No Videos Found
        each item in videos
            +videoBlock({
                videoFile : item.videoFile,
                title : item.title,
                views : item.views,
                id : item.id
            })

            //- same structure home